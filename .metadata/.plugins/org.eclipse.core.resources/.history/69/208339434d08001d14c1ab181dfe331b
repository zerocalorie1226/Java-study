package code;

public class Example5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i;
		int j;

		for(i=1;i<=7;i++)
		{
			
			for(j=1;j<=i;j++)
			{
				if(j%2==1)
				{
					System.out.print("*");
				}
				else 
				{
					continue;
				}
			
			}
			System.out.println("");
		}
	}

}
